@model ExploreModel
@{

    ViewData["Title"] = "Explore";

}


<div class="content-container">

@if (Model != null)
{
    var res = Model.Result.First();
    Model.Result.RemoveAll(m => m.Url == res.Url);
    
    <div>
        @res.Title
    </div>
    
    <iframe id="content"
            sandbox="allow-same-origin allow-scripts allow-popups allow-forms"
            frameborder="0"
            width="100%"
            height="100%"
            seamless="seamless"
            src=@res.Url>
    </iframe>
    
}

</div>
