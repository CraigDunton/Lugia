@model ExploreModel
@{
    ViewData["Title"] = "Explore";
    //TempData["Query"] = Model.Query;
    //TempData["Result"] = Model.Result;
    //Console.WriteLine("%$%$%$%$HERE: {0}, {1}", Model.Query, Model.Result);
}


<div class="content-container">

@if (Model != null)
{
    //TODO: make this take a random URL for more diversity
    var res = Model.Result.First();
    
    <div>
        @res.Title
    </div>
    
    <iframe id="content"
            sandbox="allow-same-origin allow-scripts allow-popups allow-forms"
            frameborder="0"
            width="100%"
            height="100%"
            seamless="seamless"
            src=@res.Url>
    </iframe>
    
}

</div>
